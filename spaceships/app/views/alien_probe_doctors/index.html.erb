<h2>Listing Alien Probe Doctors</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alien_probe_doctors.each do |alien_probe_doctor| %>
      <tr>
        <td><%= alien_probe_doctor.name %></td>
        <td colspan="3"></td>


        <td><%= link_to 'Show', alien_probe_doctor %></td>
        <td><%= link_to 'Edit', edit_alien_probe_doctor_path(alien_probe_doctor) %></td>
        <td><%= link_to 'Destroy', alien_probe_doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alien probe doctor', new_alien_probe_doctor_path %>
